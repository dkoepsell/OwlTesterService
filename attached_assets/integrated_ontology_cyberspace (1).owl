<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.example.org/ontology/cyberspace#"
     xml:base="http://www.example.org/ontology/cyberspace"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:cyberspace="http://www.example.org/ontology/cyberspace#"
     xmlns:bfo="http://www.ifomis.org/bfo/1.1#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">

    <owl:Ontology rdf:about="http://www.example.org/ontology/cyberspace">
        <dc:title>Integrated Legal and BFO Ontology of Cyberspace</dc:title>
        <dc:creator>David Koepsell (Concepts), Generated by ChatGPT</dc:creator>
        <dc:description>An OWL-based BFO ontology combining cyberspace and Koepsell's legal classifications.</dc:description>
    </owl:Ontology>

    <!-- Core Classes -->
    <owl:Class rdf:about="bfo:MaterialEntity"/>
    <owl:Class rdf:about="bfo:DependentContinuant"/>
    <owl:Class rdf:about="bfo:Process"/>

    <!-- Material Entities -->
    <owl:Class rdf:about="cyberspace:Hardware">
        <rdfs:subClassOf rdf:resource="bfo:MaterialEntity"/>
    </owl:Class>
    <owl:Class rdf:about="cyberspace:StorageDevice">
        <rdfs:subClassOf rdf:resource="cyberspace:Hardware"/>
    </owl:Class>
    <owl:Class rdf:about="cyberspace:Processor">
        <rdfs:subClassOf rdf:resource="cyberspace:Hardware"/>
    </owl:Class>

    <!-- Immaterial Entities -->
    <owl:Class rdf:about="cyberspace:Software">
        <rdfs:subClassOf rdf:resource="bfo:DependentContinuant"/>
        <rdfs:comment>Software as legal and ontological entities.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="cyberspace:DigitalExpression">
        <rdfs:subClassOf rdf:resource="bfo:DependentContinuant"/>
    </owl:Class>

    <!-- Legal Classes -->
    <owl:Class rdf:about="cyberspace:LegalObject">
        <rdfs:subClassOf rdf:resource="bfo:DependentContinuant"/>
        <rdfs:comment>Objects that are subject to legal classification.</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="cyberspace:CopyrightableObject">
        <rdfs:subClassOf rdf:resource="cyberspace:LegalObject"/>
        <rdfs:comment>Expressions or their fixed elements protected by copyright law.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="cyberspace:PatentableObject">
        <rdfs:subClassOf rdf:resource="cyberspace:LegalObject"/>
        <rdfs:comment>Processes or machines protected under patent law.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="cyberspace:NonProtectableObject">
        <rdfs:subClassOf rdf:resource="cyberspace:LegalObject"/>
        <rdfs:comment>Ideas, concepts, or natural phenomena outside legal protection.</rdfs:comment>
    </owl:Class>
    <owl:Class rdf:about="cyberspace:TradeSecret">
        <rdfs:subClassOf rdf:resource="cyberspace:LegalObject"/>
        <rdfs:comment>Formulas, processes, or compilations that give competitive advantages.</rdfs:comment>
    </owl:Class>

    <!-- Occurrents -->
    <owl:Class rdf:about="cyberspace:InformationTransaction">
        <rdfs:subClassOf rdf:resource="bfo:Process"/>
    </owl:Class>
    <owl:Class rdf:about="cyberspace:VirtualInteraction">
        <rdfs:subClassOf rdf:resource="bfo:Process"/>
    </owl:Class>

    <!-- Relationships -->
    <owl:ObjectProperty rdf:about="cyberspace:protectedBy">
        <rdfs:domain rdf:resource="cyberspace:LegalObject"/>
        <rdfs:range rdf:resource="cyberspace:LegalProtection"/>
        <rdfs:comment>Indicates the type of legal protection applied to an object.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="cyberspace:classifiedAs">
        <rdfs:domain rdf:resource="cyberspace:Software"/>
        <rdfs:range rdf:resource="cyberspace:LegalObject"/>
        <rdfs:comment>Classifies software entities under legal categories.</rdfs:comment>
    </owl:ObjectProperty>

    <!-- Named Individuals -->
    <!-- Example of Software under Multiple Classifications -->
    <owl:NamedIndividual rdf:about="cyberspace:PGPEncryptionProgram">
        <rdf:type rdf:resource="cyberspace:Software"/>
        <cyberspace:classifiedAs rdf:resource="cyberspace:CopyrightableObject"/>
        <cyberspace:classifiedAs rdf:resource="cyberspace:PatentableObject"/>
    </owl:NamedIndividual>

    <!-- Examples of Other Entities -->
    <owl:NamedIndividual rdf:about="cyberspace:EmailMessage1234">
        <rdf:type rdf:resource="cyberspace:DigitalExpression"/>
        <cyberspace:classifiedAs rdf:resource="cyberspace:CopyrightableObject"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="cyberspace:AlgorithmSHA256">
        <rdf:type rdf:resource="cyberspace:NonProtectableObject"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="cyberspace:TradeSecretFormulaXYZ">
        <rdf:type rdf:resource="cyberspace:TradeSecret"/>
    </owl:NamedIndividual>

</rdf:RDF>
