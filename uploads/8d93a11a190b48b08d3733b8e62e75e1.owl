<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
>
  <rdf:Description rdf:about="http://example.org/legal#is_about">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>is_about</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/legal#Evidence"/>
    <rdfs:range rdf:resource="http://example.org/legal#Fact"/>
    <rdfs:comment>is_about from Evidence to Fact</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#determines">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>determines</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/legal#LegalActor"/>
    <rdfs:range rdf:resource="http://example.org/legal#LegalFact"/>
    <rdfs:comment>determines from LegalActor to LegalFact</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#LegalClaim">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>LegalClaim</rdfs:label>
    <rdfs:comment>An assertion of a right, obligation, or violation presented in a legal context.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#RealWorld">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>RealWorld</rdfs:label>
    <rdfs:comment>A reality comprising brute facts independent of institutional structures.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#LegalActor">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>LegalActor</rdfs:label>
    <rdfs:comment>A human agent bearing a legal role, such as a judge, lawyer, or officer.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#LegalArgument">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>LegalArgument</rdfs:label>
    <rdfs:comment>A reason or rationale supporting or contesting a claim or evidence.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#supports">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>supports</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/legal#Evidence"/>
    <rdfs:range rdf:resource="http://example.org/legal#LegalClaim"/>
    <rdfs:comment>supports from Evidence to LegalClaim</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#influences">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>influences</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/legal#Fact"/>
    <rdfs:range rdf:resource="http://example.org/legal#LegalCase"/>
    <rdfs:comment>influences from Fact to LegalCase</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#LegalRule">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>LegalRule</rdfs:label>
    <rdfs:comment>A prescriptive normative statement that determines legal consequences.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#Evidence">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Evidence</rdfs:label>
    <rdfs:comment>Information content that supports or contests a legal claim.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#applies_to">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>applies_to</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/legal#LegalRule"/>
    <rdfs:range rdf:resource="http://example.org/legal#LegalFact"/>
    <rdfs:comment>applies_to from LegalRule to LegalFact</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#participates_in">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>participates_in</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/legal#LegalActor"/>
    <rdfs:range rdf:resource="http://example.org/legal#LegalProcess"/>
    <rdfs:comment>participates_in from LegalActor to LegalProcess</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#LegalRole">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>LegalRole</rdfs:label>
    <rdfs:comment>A specifically dependent continuant inhering in an agent, authorized by a legal system.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#corresponds_to">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>corresponds_to</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/legal#LegalFact"/>
    <rdfs:range rdf:resource="http://example.org/legal#Fact"/>
    <rdfs:comment>corresponds_to from LegalFact to Fact</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#presents">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>presents</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/legal#LegalActor"/>
    <rdfs:range rdf:resource="http://example.org/legal#LegalEvidence"/>
    <rdfs:comment>presents from LegalActor to LegalEvidence</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#has_part">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has_part</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/legal#LegalCase"/>
    <rdfs:range rdf:resource="http://example.org/legal#LegalFact"/>
    <rdfs:comment>has_part from LegalCase to LegalFact</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#Fact">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Fact</rdfs:label>
    <rdfs:comment>A situation that obtains in the real world at a given time.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#constitutes">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>constitutes</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/legal#DocumentaryAct"/>
    <rdfs:range rdf:resource="http://example.org/legal#LegalFact"/>
    <rdfs:comment>constitutes from DocumentaryAct to LegalFact</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#involves">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>involves</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/legal#LegalCase"/>
    <rdfs:range rdf:resource="http://example.org/legal#LegalActor"/>
    <rdfs:comment>involves from LegalCase to LegalActor</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#DocumentaryAct">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>DocumentaryAct</rdfs:label>
    <rdfs:comment>A performative act using a document or record that enacts or recognizes legal facts.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#LegalOutcome">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>LegalOutcome</rdfs:label>
    <rdfs:comment>A judgment or result from a legal process.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#produces">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>produces</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/legal#LegalProcess"/>
    <rdfs:range rdf:resource="http://example.org/legal#LegalOutcome"/>
    <rdfs:comment>produces from LegalProcess to LegalOutcome</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#LegalFact">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>LegalFact</rdfs:label>
    <rdfs:comment>A legally relevant situation determined through legal processes, based on roles, norms, and evidence.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#SocialReality">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>SocialReality</rdfs:label>
    <rdfs:comment>A reality composed of entities sustained by collective intentionality.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#LegalReality">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>LegalReality</rdfs:label>
    <rdfs:comment>A subset of social reality composed of law-related entities and norms.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#LegalProcess">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>LegalProcess</rdfs:label>
    <rdfs:comment>A structured series of events (e.g., trial, appeal) involving legal roles and arguments.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#LegalEvidence">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>LegalEvidence</rdfs:label>
    <rdfs:comment>A piece of information presented in a legal process to support or refute a claim.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/legal#LegalCase">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>LegalCase</rdfs:label>
    <rdfs:comment>A process involving adjudication governed by a legal system's norms and procedures.</rdfs:comment>
  </rdf:Description>
</rdf:RDF>

